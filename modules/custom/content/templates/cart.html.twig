<div>
    <table width="100%">
    <tr><th> name </th> <th>image</th> <th>price</th> <th>category</th> <th>quantity</th> <th>remove</th> </tr>
    {% for key,ct in data %}
        {# <div> key : {{key}} </div> #}
        <tr>
        {% for d , c in ct %}
          {% if d == 1%}
            <td><img src={{c}} height="30" width="30"> </td>
						{% elseif d == 5 %}
						<td> <form> <button name = "{{c}}" value = "{{c}}"> remove </button> </form> </td>
            {% else %}
                    <td>{{c}}</td>
            {% endif %}
        {% endfor %}
        </tr>
    {% endfor %}
    </table>

</div>